<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="tireK" class="com.edu.di.car.mycar05.KoreaTire"/>
    <bean id="tireM" class="com.edu.di.car.mycar05.MexicoTire"/>
    <bean id="trieC" class="com.edu.di.car.mycar05.ChinaTire"/>
    
    <bean id="car" class="com.edu.di.car.mycar05.Car">
       <property name="tire" ref="tireM"></property>
    </bean>
    
</beans>
